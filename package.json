{
  "name": "angular-schema-form",
  "version": "1.0.0-alpha.2",
  "description": "Create complex forms from a JSON schema with angular.",
  "repository": "json-schema-form/angular-schema-form",
  "main": "dist/schema-form.js",
  "filename": "dist/schema-form.min.js",
  "homepage": "http://schemaform.io",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "minify": "uglifyjs dist/schema-form.js --comments --output=dist/schema-form.min.js",
    "dist": "npm run build && npm run minify",
    "testlinux": "rm -fr coverage && ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS karma.conf.js && find coverage/ -name lcov.info -print0 | xargs -0 cat | ./node_modules/coveralls/bin/coveralls.js",
    "test": "karma start karma.conf.js --log-level debug"
  },
  "author": "json-schema-form",
  "contributors": [
    "David Jensen <david.lgj@gmail.com> (https://github.com/davidlgj)",
    "Marcel J Bennett <iamanthropic@gmail.com> (https://github.com/Anthropic)",
    "Denis Dervisevic <denis@dervisevic.se> (https://github.com/Dervisevic)",
    "Cameron Edwards (https://github.com/cameronprattedwards)",
    "Mike Marcacci (https://github.com/mike-marcacci)"
  ],
  "license": "MIT",
  "dependencies": {
    "angular": ">= 1.2",
    "tv4": "~1.0.15",
    "angular-sanitize": ">= 1.2"
  },
  "keywords": [
    "angular",
    "angularjs",
    "form",
    "json",
    "json-schema",
    "schema"
  ],
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.0",
    "gulp": "^3.5.6",
    "gulp-jscs": "^1.1.0",
    "json-schema-form-core": "git://github.com/json-schema-form/json-schema-form-core.git#develop",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.0.0",
    "karma-growler-reporter": "0.0.1",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "0.0.1",
    "protractor": "^2.5.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "streamqueue": "0.0.5",
    "uglify-js": "^2.6.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/json-schema-form/angular-schema-form/master/LICENSE"
    }
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/json-schema-form/angular-schema-form.git",
    "basePath": "/dist/",
    "files": [
      "schema-form.min.js",
      "schema-form.js",
      "bootstrap-decorator.min.js",
      "bootstrap-decorator.js"
    ]
  }
}
